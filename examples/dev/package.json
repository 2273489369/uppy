{
  "dependencies": {
    "aliasify": "^2.1.0",
    "babel-core": "^6.26.3",
    "babelify": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "watchify": "^3.11.0"
  },
  "scripts": {
    "watch:sandbox": "npm-run-all --parallel watch:Dashboard watch:DragDrop",
    "watch:Dashboard": "watchify -t babelify -t aliasify Dashboard.js -o lib/Dashboard.js -vd",
    "watch:DragDrop": "watchify -t babelify -t aliasify DragDrop.js -o lib/DragDrop.js -vd"
  },
  "aliasify": {
    "aliases": {
      "@uppy": "../../packages/@uppy"
    }
  }
}
